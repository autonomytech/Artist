<% if notice %>
	<%= content_tag :javascript do%>
		<script type="text/javascript">
			artist_msg("<%= notice %>");
		</script>
	<% end %>
<% end %>
<% if alert %>
	<%= content_tag :javascript do%>
		<script type="text/javascript">
			artist_msg("<%= alert %>",'error');
		</script>
	<% end %>
<% end %>