<%= bootstrap_form_for([@profile,@qualification]) do |f| %>
    <%= f.text_field :qualification, placeholder: 'Qualification' %>
    <%= f.text_field :college_name, placeholder: 'College Name' %>
    <%= f.text_field :university, placeholder: 'University' %>
    <%= f.text_field :year, placeholder: 'Year' %>
    <%= f.submit 'Save', class: 'btn btn-sm btn-primary' %>
<% end %>
