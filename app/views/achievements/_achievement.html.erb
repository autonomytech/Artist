<% @profile.achievements.each do |achievement|%>
<tr>
	<td><%= achievement.name %></td>
	<td><%= achievement.date %></td>
	<td><%= achievement.details %></td>
	<td><%= link_to edit_profile_achievement_path(@profile, achievement), remote: true do %><i class="fa fa-pencil fa-lg"></i><%end%>&nbsp;&nbsp;&nbsp;<%= link_to [@profile, achievement], method: :delete, data: { confirm: 'Are you sure?' }, remote: true do %><i class="fa fa-trash fa-lg"></i><%end%></td>
</tr>
<%end%>